<?xml version="1.0" encoding="utf-8"?>
<com.innovations.aguilar.pocketstrats.ui.MainPaneContainer
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout_main_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/colorPrimaryContrast"
    tools:context="com.innovations.aguilar.pocketstrats.ui.MainActivity">

    <com.innovations.aguilar.pocketstrats.ui.view.MainScreenView
        android:id="@+id/layout_main_screen_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@color/colorPrimaryContrast" >
        <Button
            android:id="@+id/button_modes"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Map Strategy" />
    </com.innovations.aguilar.pocketstrats.ui.view.MainScreenView>

</com.innovations.aguilar.pocketstrats.ui.MainPaneContainer>

