<?xml version="1.0" encoding="utf-8"?>
<com.innovations.aguilar.pocketstrats.ui.view.MapSubjectsView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/layout_map_subjects"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.innovations.aguilar.pocketstrats.ui.view.MapSpawnTabLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/map_spawn_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">


        <!--android:divider="@null"-->
        <!--android:dividerHeight="0dp"-->
        <android.support.v7.widget.RecyclerView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/list_subjects"
            android:groupIndicator="@drawable/tips_expandable_group"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <com.innovations.aguilar.pocketstrats.ui.view.MapSubjectsDetailsView
            android:id="@+id/view_subject_details"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        </com.innovations.aguilar.pocketstrats.ui.view.MapSubjectsDetailsView>
    </com.innovations.aguilar.pocketstrats.ui.view.MapSpawnTabLayout>

</com.innovations.aguilar.pocketstrats.ui.view.MapSubjectsView>